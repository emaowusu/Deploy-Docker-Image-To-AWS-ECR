{
  "family": "secret-gen-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::819312869829:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "secret-gen-container",
      "image": "819312869829.dkr.ecr.us-east-1.amazonaws.com/secret-gen-repo:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
